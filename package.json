{
  "name": "root-workspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn data; next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "yarn rome:format",
    "rome:format": "rome format --write *.{ts,tsx,js} src workspaces/*/{src,*.js,.storybook}",
    "rome:check": "rome check *.{ts,tsx,js} src workspaces/*/{src,*.js,.storybook}",
    "eslint:check": "eslint *.{ts,tsx,js} src workspaces/*/{src,*.js,.storybook}",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "yarn vitest run",
    "test:e2e": "start-server-and-test 'yarn dev' 3000 'yarn cy:open'",
    "test:e2e:ci": "start-server-and-test 'yarn dev' 3000 'yarn cy:run'",
    "typecheck": "tsc --noEmit",
    "data": "yarn workspace content-onedge-workspace update-dynamic-data",
    "storybook:dev": "yarn workspace storybook-workspace dev",
    "storybook:build": "yarn workspace storybook-workspace build",
    "storybook": "yarn workspace storybook-workspace storybook",
    "build-storybook": "yarn workspace storybook-workspace build-storybook"
  },
  "dependencies": {
    "@chakra-ui/pro-theme": "^0.0.57",
    "@chakra-ui/react": "^2.4.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/inter": "^4.5.14",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@mdx-js/loader": "^2.2.1",
    "@mdx-js/mdx": "^2.2.1",
    "@mdx-js/react": "^2.2.1",
    "@netlify/plugin-nextjs": "^4.30.0",
    "@next/mdx": "^13.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/mdx": "^2.0.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "algoliasearch": "^4.14.3",
    "autoprefixer": "^10.4.13",
    "browser-lang": "0.1.0",
    "classnames": "^2.3.2",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "framer-motion": "^6",
    "googleapis": "^110.0.0",
    "happy-dom": "^8.1.3",
    "js-yaml-loader": "^1.2.2",
    "next": "^13.1.1",
    "next-intl": "next",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-instantsearch-hooks-web": "^6.38.3",
    "react-use": "^17.4.0",
    "remark-frontmatter": "^4.0.1",
    "remark-mdx-frontmatter": "^2.1.1",
    "rome": "^11.0.0",
    "start-server-and-test": "^1.15.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.0",
    "yaml": "^2.2.1"
  },
  "packageManager": "yarn@3.3.1",
  "workspaces": [
    "./workspaces/*"
  ],
  "resolutions": {
    "netlify-cms-widget-markdown/unified": "^10.0.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0"
  }
}
